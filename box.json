{
    "alias": "symfony-certification.phar",
    "output": "symfony-certification.phar",
    "chmod": "0755",
    "stub": true,
    "directories": [
        "src",
        "data"
    ],
    "files": [
        "composer.json",
        "bin/exam-start"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "tests",
                "vendor"
            ],
            "in": "vendor"
        }
    ],
    "main": "bin/exam-start",
    "compression": "GZ"
}
